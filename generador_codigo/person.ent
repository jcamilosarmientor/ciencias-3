entity Persona {
  nombre : string       // Nombre de la persona
  apellido : string       // Apellido de la persona
  direccion: Direccion    // It is defined by the Comment rule in the grammar
  edad: integer        // integer and string are built-in objects
  telefono : Telefono       // Nombre de la persona
  correo : string       // mail de la persona
}                     // See entity_test.py

entity Direccion {
  calle : string
  ciudad : string
  pais : string
}

entity Telefono {
  indicador : string
  numero : string
}

entity Alien {
  planeta: string
  color: string
  estrella: integer
  planeta: Planeta
}

entity Planeta {
  color: string
  lunas: bool 
  estrellas: bool
}